                         Text="{Binding Path=GlycanMakerModel.NumberOfAla_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="arg_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="80,0,0,120" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfArg_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="asn_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="80,0,0,53" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfAsn_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="asp_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="80,14,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfAsp_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="cys_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="80,81,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfCys_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="gln_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="80,148,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfGln_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="glu_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="80,215,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfGlu_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="gly_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="190,0,0,187" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfGly_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="his_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="190,0,0,120" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfHis_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="ile_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="190,0,0,53" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfIle_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="leu_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="190,14,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfLeu_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="lys_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="190,81,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfLys_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="met_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="190,148,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfMet_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="phe_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="190,215,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfPhe_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="ser_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="300,0,0,187" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfSer_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="thr_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="300,0,0,120" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfThr_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="trp_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="300,0,0,53" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfTrp_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="tyr_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="300,14,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfTyr_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="val_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="300,81,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfVal_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Column="1" Name="pro_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="300,148,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfPro_Int}" TextChanged="Hexose_UTextBox_TextChanged" />


                <!--"Permethyl" Label (Row 2 x Col 2)-->
                <Label Grid.Column="1" Grid.Row="1" Content="Permethyl" 
                Style="{StaticResource tertiaryHeaderLabelTextStyle}" />

                <!--Permethyl White Outline-->
                <Path Grid.Column="1" Grid.Row="1" Stroke="White" StrokeThickness="2" 
                Data="M 125,18 H 10 V 175 H 335 V 18 H 215" />

                <!--Permethyl CheckBoxes-->
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pHex_UCheckBox" Margin="15,0,0,95" VerticalAlignment="Center" 
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpHex_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pHxNAc_UCheckBox" Margin="15,0,0,25" VerticalAlignment="Center" 
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpHxNAc_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pDxHex_UCheckBox" Margin="15,45,0,0" VerticalAlignment="Center" 
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpDxHex_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pPntos_UCheckBox" Margin="15,135,0,0" VerticalAlignment="Center"
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpPntos_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pNuAc_UCheckBox" Margin="185,0,0,95" VerticalAlignment="Center" 
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpNuAc_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pNuGc_UCheckBox" Margin="185,0,0,25" VerticalAlignment="Center" 
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpNuGc_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pKDN_UCheckBox" Margin="185,45,0,0" VerticalAlignment="Center" 
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpKDN_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                <CheckBox Grid.Row="1" Grid.Column="1" Name="pHxA_UCheckBox" Margin="185,135,0,0" VerticalAlignment="Center" 
                          IsChecked="{Binding Path=OmniFinderGMModel.CheckedpHxA_Bool, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <!--Permethyl Labels-->
                <Label Grid.Row="1" Grid.Column="1" Content="pHex" Margin="30,0,0,95" Style="{StaticResource blackLeftLineLabelTextStyle}"/>
                <Label Grid.Row="1" Grid.Column="1" Content="pHxNAc" Margin="30,0,0,25" Style="{StaticResource blackLeftLineLabelTextStyle}"/>
                <Label Grid.Row="1" Grid.Column="1" Content="pDxHex" Margin="30,45,0,0" Style="{StaticResource blackLeftLineLabelTextStyle}"/>
                <Label Grid.Row="1" Grid.Column="1" Content="pPntos" Margin="30,135,0,0" Style="{StaticResource blackLeftLineLabelTextStyle}" />
                <Label Grid.Row="1" Grid.Column="1" Content="pNuAc" Margin="200,0,0,95" Style="{StaticResource blackLeftLineLabelTextStyle}"/>
                <Label Grid.Row="1" Grid.Column="1" Content="pNuGc" Margin="200,0,0,25" Style="{StaticResource blackLeftLineLabelTextStyle}"/>
                <Label Grid.Row="1" Grid.Column="1" Content="pKDN" Margin="200,45,0,0" Style="{StaticResource blackLeftLineLabelTextStyle}"/>
                <Label Grid.Row="1" Grid.Column="1" Content="pHxA" Margin="200,135,0,0" Style="{StaticResource blackLeftLineLabelTextStyle}"/>

                <!--"#" Labels-->
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="90,0,0,95" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="90,0,0,25" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="90,45,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="90,135,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="250,0,0,95" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="250,0,0,25" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="250,45,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="1" Grid.Column="1" Content="#" Margin="250,135,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" />

                <!--Permethyl Text Boxes-->
                <TextBox Grid.Row="1" Grid.Column="1" Name="pHex_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="110,0,0,95" Width="25"
                         Text="{Binding Path=GlycanMakerModel.NumberOfpHex_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Row="1" Grid.Column="1" Name="pHxNAc_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="110,0,0,25" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfpHxNAc_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Row="1" Grid.Column="1" Name="pDxHex_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="110,45,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfpDxHex_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Row="1" Grid.Column="1" Name="pPntos_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="110,135,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfpPntos_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Row="1" Grid.Column="1" Name="pNuAc_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="270,0,0,95" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfpNuAc_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Row="1" Grid.Column="1" Name="pNuGc_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="270,0,0,25" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfpNuGc_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Row="1" Grid.Column="1" Name="pKDN_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="270,45,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfpKDN_Int}" TextChanged="Hexose_UTextBox_TextChanged" />
                <TextBox Grid.Row="1" Grid.Column="1" Name="pHxA_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="270,135,0,0" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfpHxA_Int}" TextChanged="Hexose_UTextBox_TextChanged" />


                <!--Charge Carrier White Outline (Row 3 x Col 1)-->
                <Path Grid.Row="2" Grid.Column="0" Stroke="White" StrokeThickness="2" Data="M 10,5 V 155 H 285 V 5 H 10" />

                <!--"Charge Carrier" and "Carbohydrate Type" Labels-->
                <Label Grid.Row="2" Grid.Column="0" Content="Charge Carrier" Margin="10,0,0,120" Style="{StaticResource leftLineLabelTextStyle}" />
                <Label Grid.Row="2" Grid.Column="0" Content="Carbohydrate Type" Margin="10,20,0,0" Style="{StaticResource leftLineLabelTextStyle}" />

                <!--Charge Carrier Combobox-->
                <ComboBox Grid.Row="2" Grid.Column="0" Name="chargeCarrierGlycanMakerComboBox"  Height="23" Width="70" Margin="20,0,0,50"
                          HorizontalAlignment="Left" VerticalAlignment="Center" IsEditable="true" IsReadOnly="true"
                          ItemsSource="{StaticResource ChargeCarrierStrings}" DisplayMemberPath="ValueString" SelectedValuePath="ValueString" 
                          SelectedValue="{Binding Path=GlycanMakerModel.TypeOfChargeCarrier_String, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                <!--Charge Carrier "#" Label and corresponding Text Box-->
                <Label Grid.Row="2" Content="#" Margin="150,0,0,50" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <TextBox Grid.Row="2" Name="ChargeCarrier_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="170,0,0,50" Width="25" 
                         Text="{Binding Path=GlycanMakerModel.NumberOfChargeCarrier_Int}" TextChanged="Hexose_UTextBox_TextChanged" />


                <!--Carbohydrate Type Combobox-->
                <ComboBox Grid.Row="2" Grid.Column="0" Name="carbohydrateTypeGlycanMakerComboBox"  Height="23" 
                          HorizontalAlignment="Left" VerticalAlignment="Center" Width="130" Margin="20,80,0,0"
                          IsEditable="true" IsReadOnly="true" ItemsSource="{StaticResource CarbTypeStrings}" 
                          DisplayMemberPath="ValueString" SelectedValuePath="ValueString" 
                          SelectedValue="{Binding Path=GlycanMakerModel.CarbohydrateType_String, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />


                <!-- Neutral Mass White Outline (Row 3 x Col 2)-->
                <Path Grid.Row="2" Grid.Column="1" Stroke="White" StrokeThickness="2" Data="M 5,5 V 155 H 335 V 5 H 5" />

                <!--"Neutral Mass" and "Mass/Charge" Labels-->
                <Label Grid.Row="2" Grid.Column="1" Content="Neutral Mass" Margin="10,0,0,110" Style="{StaticResource leftLineLabelTextStyle}" />
                <Label Grid.Row="2" Grid.Column="1" Content="Mass/Charge" Margin="10,0,0,50" Style="{StaticResource leftLineLabelTextStyle}" />

                <!--"->" Labels in Front of Neutral Mass and Mass/Change Text Boxes-->
                <Label Grid.Row="2" Grid.Column="1" Content="-->" Margin="120,0,0,110" HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Label Grid.Row="2" Grid.Column="1" Content="-->" Margin="120,0,0,50" HorizontalAlignment="Left" VerticalAlignment="Center" />

                <!--Neutral Mass and Mass/Charge Text Boxes-->
                <TextBox Grid.Row="2" Grid.Column="1" Name="neutralMass_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="170,0,0,110" 
                         Width="100" IsReadOnly="True" Text="{Binding Path=GlycanMakerModel.NeutralMass_Double}" />
                <TextBox Grid.Row="2" Grid.Column="1" Name="massCharge_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="170,0,0,50" 
                         Width="100" IsReadOnly="True" Text="{Binding Path=GlycanMakerModel.MassCharge_Double}" />


                <!--"C, H, O, N, and Na" Labels-->
                <Label Grid.Row="2" Grid.Column="1" Content="C" Margin="0,15,190,0" Style="{StaticResource directionsLabelTextStyle}" />
                <Label Grid.Row="2" Grid.Column="1" Content="H" Margin="0,15,25,0" Style="{StaticResource directionsLabelTextStyle}" />
                <Label Grid.Row="2" Grid.Column="1" Content="O" Margin="140,15,0,0" Style="{StaticResource directionsLabelTextStyle}" />
                <Label Grid.Row="2" Grid.Column="1" Content="N" Margin="0,85,190,0" Style="{StaticResource directionsLabelTextStyle}" />
                <Label Grid.Row="2" Grid.Column="1" Content="Na" Margin="0,85,20,0" Style="{StaticResource directionsLabelTextStyle}" />

                <!--C, H, O, N, and Na Amount Text Boxes-->
                <TextBox Grid.Row="2" Grid.Column="1" Name="C_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="35,15,0,0" Width="25"
                         IsReadOnly="True" Text="{Binding Path=GlycanMakerModel.NumberOfC_Int}" />
                <TextBox Grid.Row="2" Grid.Column="1" Name="H_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="120,15,0,0" Width="25"
                         IsReadOnly="True" Text="{Binding Path=GlycanMakerModel.NumberOfH_Int}" />
                <TextBox Grid.Row="2" Grid.Column="1" Name="O_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="205,15,0,0" Width="25" 
                         IsReadOnly="True" Text="{Binding Path=GlycanMakerModel.NumberOfO_Int}" />
                <TextBox Grid.Row="2" Grid.Column="1" Name="N_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="35,85,0,0" Width="25" 
                         IsReadOnly="True" Text="{Binding Path=GlycanMakerModel.NumberOfN_Int}" />
                <TextBox Grid.Row="2" Grid.Column="1" Name="Na_UTextBox" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="120,85,0,0" Width="25"
                         IsReadOnly="True" Text="{Binding Path=GlycanMakerModel.NumberOfNa_Int}" />
            </Grid>


            <!--Calculate Mass Button (Row 3)-->
            <Button Grid.Row="2" Name="calculateMassButton" Content="Calculate Mass" Margin="0,0,350,0" Width="180"
                    Style="{StaticResource optionButtonStyle}" Command="{Binding ButtonCommand}" CommandParameter="calculateMass">
                <Button.Background>
                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                        <GradientStop Color="LightSkyBlue" Offset="0.25"/>
                        <GradientStop Color="MediumSlateBlue" Offset="1.0"/>
                    </LinearGradientBrush>
                </Button.Background>
            </Button>


            <!--Ranges Button-->
            <Button Grid.Row="2" Name="rangesButton" Content="Ranges" Margin="300,0,0,0" Width="140"
                    Style="{StaticResource optionButtonStyle}" Click="rangesButton_Click">
                <Button.Background>
                    <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                        <GradientStop Color="Pink" Offset="0.5"/>
                        <GradientStop Color="DeepPink" Offset="1.0"/>
                    </LinearGradientBrush>
                </Button.Background>
            </Button>

        </Grid>
    </Canvas>
</Page>
