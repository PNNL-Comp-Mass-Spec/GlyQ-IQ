                      ItemsSource="{StaticResource OmniFinderStrings}" DisplayMemberPath="ValueString" SelectedValuePath="ValueString" 
                      SelectedValue="{Binding Path=OmniFinderGMModel.SelectedOption_String, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
            

